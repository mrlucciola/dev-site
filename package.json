    "redux-thunk": "^2.3.0",
    "web-vitals": "^2.1.4"
  },
  "description": "Portfolio page for Rocco Lucciola",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "move": "rm -rf ./static ./assets ./images ; mv -f ./build/* ./ && rm -rf ./build ; cp -r ./assets/* ./",
    "git-add": "git add .",
    "git-commit": "git commit -m '(re)building and deploying'",
    "git-push": "git push origin gh-pages",
    "git-deploy": "npm run git-add && npm run git-commit && npm run git-push",
    "deploy": "npm run build && npm run move && npm run git-deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrlucciola/dev-site.git"
  },
  "author": "Rocco Lucciola",
  "license": "MIT",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/node": "^16.11.47",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "typescript": "^4.7.4"
  }
